<apex:page standardController="Account" tabStyle="Account" action="{!redirectView}" extensions="VFCE05_AccountViewEditOverride">
<apex:includeScript value="/support/console/20.0/integration.js"/>
<apex:pageMessages ></apex:pageMessages>
<apex:form >
    <apex:inputhidden value="{!Account.ownerid}"/>
    <apex:inputhidden value="{!Account.State__c}"/>
    <apex:inputhidden value="{!Account.Company_State__c}"/>
    <apex:inputhidden value="{!Account.recordTypeId}"/>
    <apex:inputhidden value="{!Account.Tech_Businesstrack__c}"/>
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<apex:commandButton value="Back" onclick="testCloseTab();return false" />
</apex:form>
<script type="text/javascript">
    function testCloseTab() {
        //First find the ID of the current tab to close it
        sforce.console.getEnclosingTabId(closeSubtab);
    }
    var closeSubtab = function closeSubtab(result) {
        //Now that we have the tab ID, we can close it
        var tabId = result.id;
        sforce.console.closeTab(tabId);
    };
</script>
</apex:page>